@report{usmani_fortunes_2020,
  title = {The {{Fortunes}} of {{Labor}}: {{Bringing Capitalism Back In}}},
  author = {Usmani, Adaner},
  date = {2020-06-13},
  url = {https://www.dropbox.com/s/wgph6kevikxeb8w/_fortunesoflabor.pdf?dl=0},
  abstract = {Why are labor movements stronger in some places and at some times than at other places at other times? Scholars have given a variety of reasons for workers’ varying success, but the consensus view is ecumenical. Many different factors are said to matter; none has any obvious priority. I give reasons to doubt the ecumenical view. I observe that workers organize in an economic system in which employers have varying political leverage over the state, and in which competition between firms threatens unions’ longevity. Of course, union membership has grown as capitalism has advanced, so this raises the question: if capitalism gives employers the tools to undermine unions, why have unions grown as capitalism has consolidated? I propose that this is because capitalist competition also has unintended consequences that can incubate workers’ capacities for collective action. Competition between capitalists raises standards of living and, at times but not always, aggregates workers in capacity- enhancing employment. My provocation is that tactical, cultural, and institutional factors are usually proximate mechanisms for these more fundamental causes. Thus, for the big questions that interest scholars on labor, we ought to bring capitalism back into the scholarship on labor. I close by noting what my criticism implies for scholars of social movements more generally.},
  langid = {english},
  keywords = {_tablet},
  file = {/home/coyu3/Zotero/storage/JGTURU7N/Usmani_2020_The Fortunes of Labor.pdf}
}

@report{usmani_rise_2017,
  type = {SSRN Scholarly Paper},
  title = {The {{Rise}} and {{Fall}} of {{Labor}}},
  author = {Usmani, Adaner},
  date = {2017-02-15},
  number = {ID 3025799},
  institution = {{Social Science Research Network}},
  location = {{Rochester, NY}},
  doi = {10.2139/ssrn.3025799},
  abstract = {Labor unions exist everywhere, but their strength varies widely by time and by place. It is widely known that levels of unionization have fallen from their peak in the advanced world, and our best evidence suggests that unions never attained these peak levels of influence in late developers. This paper reconsiders explanations of union strength with a view to accounting for these longitudinal and cross-sectional differences. I identify four broad accounts, but note empirical, conceptual, and methodological weaknesses in existing research. I marshal as-yet unused data from the first half of the 20th century, propose an improved measure of ordinary individuals’ disruptive capacity, and employ counterfactual exercises in an effort to improve upon past work. In panel regressions spanning the modern history of the labor movement, I find strong evidence for institutionalist and especially structuralist explanations of union success. The counterfactual exercises illustrate the primacy of the structuralist account. The loss of disruptive capacity explains a larger share of union decline in the advanced world than does the loss of enabling institutions, and the structural handicaps of late industrialization explain the majority of the gap in unionization between early and late developers.},
  langid = {english},
  keywords = {_tablet},
  file = {/home/coyu3/Zotero/storage/6TP2WSTM/Usmani_2017_The Rise and Fall of Labor.pdf;/home/coyu3/Zotero/storage/GNU74B6N/papers.html}
}

