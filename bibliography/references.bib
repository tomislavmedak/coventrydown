@article{reitan_climate_2012,
  title = {Climate {{Change}} or {{Social Change}}? {{Environmental}} and {{Leftist Praxis}} and {{Participatory Action Research}}},
  shorttitle = {Climate {{Change}} or {{Social Change}}?},
  author = {Reitan, Ruth and Gibson, Shannon},
  date = {2012-06-01},
  journaltitle = {Globalizations},
  volume = {9},
  number = {3},
  pages = {395--410},
  issn = {1474-7731},
  doi = {10.1080/14747731.2012.680735},
  abstract = {This work seeks to advance and synthesize leftist political and environmental theory and enhance historical and contemporary understanding of environmental activism, especially related to the movement to halt climate change. We share findings from our participatory action research (PAR) on the internal dynamics and debates among the three main climate activist networks mobilizing at the United Nations Framework Convention on Climate Change meetings in Copenhagen in 2009, those of the reformist Climate Action Network (CAN), the more marxian-inspired Climate Justice Now! (CJN), and the autonomist and anarchistic Climate Justice Action (CJA). We elucidate how leftist reformist and especially the more radical currents are engaging with non- or anti-Western indigenous praxis to forge the contemporary transnational environmental movement. Este trabajo busca avanzar y sintetizar la teoría política izquierdista y la medioambiental y enriquecer el entendimiento histórico y contemporáneo del activismo medioambiental, especialmente relacionado al movimiento para detener el cambio climático. Compartimos los resultados de nuestra acción de investigación participativa (PAR, por sus siglas en inglés) sobre las dinámicas internas y los debates entre las tres principales redes activistas del cambio climático, movilizadas en reuniones dentro de la Convención Marco de las Naciones Unidas sobre el cambio climático en Copenhague en 2009, en aquellas reformistas de la Red de Acción Climática (CAN, por sus siglas en inglés), las de mayor inspiración marxiana de ¡Justicia Climática Ahora! (CJN, por sus siglas en inglés) y la autonomista y anarquista Acción por la Justicia Climática (CJA, por sus siglas en inglés). Dilucidamos cómo las corrientes reformistas de izquierda y especialmente las más radicales, están colaborando con las prácticas indígenas no-o-anti-occidentales para crear el movimiento medioambiental trasnacional. 本文旨在推进和综合左派政治理论和环境理论研究，并加强对环境行动主义，特别是涉及遏制气候变化运动的历史理解和当代理解。本文分享了我们对内部动态的参与行动研究（PAR）和关于2009年联合国气候变化框架公约哥本哈根会议上活跃的三个主要气候问题活动网络间争论的研究发现。三个网络分别是改革派的气候变化行动网络（CAN）、更具马克思主义倾向的气候正义现在就要！（CJN）和自治的无政府主义的气候正义行动（CJA）。我们阐述了左倾改革派、特别是更激进的派别是如何参与到非西方或反西方的本地实践中以推进当代跨国环保运动的。 이 논문은 좌파 정치이론과 환경 이론을 발전시키고 종합하고자 하며 또한 기후변화를 막기 위한 운동과 관련된 환경운동의 역사적 그리고 현재적 이해를 높이고자 한다. 우리는 2009년 코펜하게에서 개최된 기후변화에 관한 UN회의에서 동원된 3개의 주된 기후변화 반대 운동가 네트워크들 간의 내적인 동학과 논쟁에 대한 참영행동연구(PAR) 결과른 공유한다. 이들 3개 기후변화 반대 운동은 개혁주의적인 기후행동네트워크(CAN), 자율주의자와 무정부주의 기후정의행동(CJA)이다. 우리는 좌파 개혁주의자와 특히 더 급진적인 흐름들이 현대 초국적 환경운동을 만들기 위하여 어떻게 비서구 혹은 반서구 원주민 운동에 관련하였는지를 보여준다.},
  keywords = {_tablet_modified,environmentalresistance},
  file = {/home/coyu3/Zotero/storage/7MHFY9L7/Reitan_Gibson_2012_Climate Change or Social Change.pdf;/home/coyu3/Zotero/storage/9EJZE6GD/Reitan_Gibson_2012_Climate Change or Social Change.pdf;/home/coyu3/Zotero/storage/6IFRTJTX/14747731.2012.html;/home/coyu3/Zotero/storage/TR5JHA3A/14747731.2012.html}
}

