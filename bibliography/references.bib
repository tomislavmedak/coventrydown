@article{luo_does_2015,
  title = {Does {{National Culture Influence Corporate Carbon Disclosure Propensity}}?},
  author = {Luo, Laura Le and Tang, Qingliang},
  date = {2015-04-01},
  journaltitle = {Journal of International Accounting Research},
  shortjournal = {Journal of International Accounting Research},
  volume = {15},
  number = {1},
  pages = {17--47},
  issn = {1542-6297},
  doi = {10.2308/jiar-51131},
  url = {http://www.aaajournals.org/doi/abs/10.2308/jiar-51131},
  urldate = {2018-05-29},
  abstract = {This study examines the influence of culture on management's response to the challenge of climate change, as manifested in firms' voluntary participation in carbon disclosure via the Carbon Disclosure Project (CDP). We argue that national culture impacts managerial attitudes and philosophies about environmental protection and thus affects the willingness as well as the extent to which managers recognize the need for emissions control and disclosure. Based on a sample of 1,762 firms from 33 countries, we find that cultural dimensions of masculinity, power distance, and uncertainty avoidance are strongly and consistently related to carbon disclosure propensity, regardless of whether G. Hofstede, G. J. Hofstede, and Minkov (2010) or Global Leadership and Organizational Behaviour Effectiveness (GLOBE) culture measures are used. Our results also show individualism and long-term orientation has significant impact under the Hofstede measure, although not under GLOBE measures, after controlling for other compounding factors. In addition, our evidence implies that national culture may moderate the effect of carbon control mechanisms, such as emissions trading schemes. Finally, the empirical evidence indicates that the impact of culture is not sensitive to national wealth and industry membership. The findings suggest culture exerts incremental influences beyond economic and regulatory incentives and therefore should be adequately considered in the combat against global warming and particularly in negotiations for an international climate agreement that is more acceptable to societies with disparate cultural backgrounds.},
  file = {/home/coyu3/Zotero/storage/S3CC3FC8/Luo_Tang_2015_Does National Culture Influence Corporate Carbon Disclosure Propensity.pdf;/home/coyu3/Zotero/storage/T5EG6MJ8/Luo_Tang_2015_Does National Culture Influence Corporate Carbon Disclosure Propensity.pdf;/home/coyu3/Zotero/storage/K6UWM5SV/jiar-51131.html;/home/coyu3/Zotero/storage/Q8PPDNPC/jiar-51131.html}
}

