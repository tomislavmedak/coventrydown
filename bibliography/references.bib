@article{dafermos_wall_2021,
  title = {The {{Wall Street Consensus}} in Pandemic Times: What Does It Mean for Climate-Aligned Development?},
  shorttitle = {The {{Wall Street Consensus}} in Pandemic Times},
  author = {Dafermos, Yannis and Gabor, Daniela and Michell, Jo},
  date = {2021-01-15},
  journaltitle = {Canadian Journal of Development Studies / Revue canadienne d'études du développement},
  volume = {0},
  number = {0},
  pages = {1--14},
  publisher = {{Routledge}},
  issn = {0225-5189},
  doi = {10.1080/02255189.2020.1865137},
  abstract = {The COVID-19 pandemic has reinforced the dominance of what Daniela Gabor calls the Wall Street Consensus (WSC) as the hegemonic approach to sustainable development. Public commitments to “green recoveries” and climate resilience, growing fiscal deficits in the Global South, and new central bank emergency liquidity measures have created more space for WSC policies. We examine the key WSC climate policy tools – climate infrastructure as an asset class, climate rescuer of last resort, disclosure of climate-related financial risks and carbon pricing – and argue that these will increase financial vulnerability in the Global South while doing little to achieve climate-aligned development.},
  annotation = {\_eprint: https://doi.org/10.1080/02255189.2020.1865137},
