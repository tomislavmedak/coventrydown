@article{roberts_idea_2017,
  title = {The {{Idea}} of {{Emancipation}} after {{Postcolonial Theory}}},
  author = {Roberts, William Clare},
  date = {2017-08-18},
  journaltitle = {Interventions},
  volume = {19},
  number = {6},
