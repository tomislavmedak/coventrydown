  title = {Epistemic {{Disobedience}}, {{Independent Thought}} and {{Decolonial Freedom}}},
  author = {Mignolo, Walter D.},
  date = {2009-12-01},
  journaltitle = {Theory, Culture \& Society},
  shortjournal = {Theory Culture Society},
  volume = {26},
