  file = {/home/coyu3/Zotero/storage/B3S2VUDF/Lordon_2010_Capitalisme, désir et servitude.pdf;/home/coyu3/Zotero/storage/NLKJ5P5A/Lordon_2010_Capitalisme, désir et servitude.pdf}
}

@book{lordon_willing_2014,
  title = {Willing {{Slaves}} of {{Capital}}: {{Spinoza}} and {{Marx}} on {{Desire}}},
